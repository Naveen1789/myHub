package introduction.welcomeToJava;

import java.util.HashMap;
import java.util.LinkedHashMap;
import java.util.Map;

public class Solution {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Hello World.\nHello Java.");
		
		Map<String, Double> linkedHashMap = new LinkedHashMap<String, Double>();
		linkedHashMap.put("M", 5.00);
		linkedHashMap.put("N", 6.00);
		linkedHashMap.put("A", 1.00);
		linkedHashMap.put("B", 2.00);
		linkedHashMap.put("C", 3.00);
		linkedHashMap.put("D", 4.00);
		
		Map<String, Double> hashMap = new HashMap<String, Double>();

		hashMap.put("X", 8.00);
		hashMap.put("E", 5.00);
		hashMap.put("F", 6.00);
		hashMap.put("G", 7.00);
		hashMap.put("H", 8.00);
		hashMap.put("I", 5.00);
		hashMap.put("J", 6.00);
		hashMap.put("K", 7.00);
		hashMap.put("L", 8.00);
		
		for (String key : hashMap.keySet()) {
            System.out.println(key + ":\t" + hashMap.get(key));
		}
	}

}
