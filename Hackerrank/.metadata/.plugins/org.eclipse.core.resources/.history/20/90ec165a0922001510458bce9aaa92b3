class myRegex{
	
	String pattern = "[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}";
	
	public static void temp(){
		Pattern p = Pattern.compile("([0-9]{1,3}\\.)([0-9]{1,3}\\.)([0-9]{1,3}\\.)([0-9]{1,3})");
		Matcher m = p.matcher("12356.456.789.90000");
		
		
		while(m.find()){
			
			System.out.println(m.group());
		}
		
	}

}
