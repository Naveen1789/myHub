class myRegex{
	
	String pattern = "^([0-9]{1,3}\\.)([0-9]{1,3}\\.)([0-9]{1,3}\\.)([0-9]{1,3})$";
	
	public static void temp(){
		Pattern p = Pattern.compile("^(((0)?(0-9)?(0-9))|((1)(0-9)(0-9))| ((2)(((0-4)(0-9))|((5)(0-6)))))$");
		Matcher m = p.matcher("356.456.789.");
		
		while(m.find()){
			System.out.println(m.group());
		}
		
	}

}


